<!-- 查询post请求结果返回页面 -->
<h1>搜索结果</h1>
<div class="result">
    <% if(searchResult !=""){ %>
        <div class="restaueant">
            <a href="<%= searchResult[0]._id %>">
                <div><img src="/img/restaurant/<%= searchResult[0].image %>" alt=""> </div>
                <h4><%= searchResult[0].name %></h4>
                <h4><%= searchResult[0].featured %></h4>
                <h4><%= searchResult[0].averageCost %></h4>
                <h4><%= searchResult[0].location %></h4>
                <h4><%= searchResult[0].promotion %></h4>
                <h4><%= searchResult[0].stars %></h4>
            </a>
        </div>
        <%}else{%>
            <p>搜索不到</p>
            <%}%>

</div>